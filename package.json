{
	"name": "@lib/db",
	"version": "1.0.0",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js"
	},
	"scripts": {
		"build": "prisma generate --schema=./src/prisma/schema.prisma && tsc",
		"push": "prisma db push --schema=./src/prisma/schema.prisma",
		"migrate": "prisma migrate dev --schema=./src/prisma/schema.prisma",
		"studio": "prisma studio --schema=./src/prisma/schema.prisma",
		"dev": "tsc --watch"
	},
	"dependencies": {
		"@prisma/adapter-pg": "^7.2.0",
		"@prisma/client": "^7.2.0",
		"dotenv": "^17.2.3"
	},
	"devDependencies": {
		"@types/node": "^25.0.9",
		"prisma": "^7.2.0",
		"typescript": "^5.9.3"
	},
	"files": [
		"dist",
		"src/prisma/schema.prisma",
		"src/prisma/generated"
	]
}
